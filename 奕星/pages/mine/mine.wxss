}
.userinfo-avatar{
width: 125px;
height: 125px;
/* margin: 10px auto 10px; */
margin-left:15px;
border-radius: 50%;
overflow: hidden;
}
.content-row{
  display: flex;
  width: 100%;
  padding:5px 10px;  
  border-bottom: 1px solid #E7E7E7;
  justify-content:space-between;
  vertical-align:middle;
}

.content-row text{
  font-size:14px;
}

.content-row icon{
  margin:-3px 20px 0 0;
  line-height: 20px;
}
.blue{
  color:#1296db;
  font-size: 18px;
}
.green{
  color:#f5871f;
  font-size: 18px;
}

.line{
  background: #E7E7E7;
  height:40rpx;
  display: flex;
  align-items:center;
}
/* 昵称 */
.myname{
  background: #E7E7E7;
  border:5px solid #ccc;
  margin:10px 20px;
	margin-left: 150px;
}
/* 余额 */
